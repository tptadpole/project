container_commands:
  00_copy_env_file:
    command: "cp .env.example .env"
    cwd: "/var/app/staging"

  01_run_key_generate:
    command: "php artisan key:generate"
    cwd: "/var/app/staging"

  02_run_storage_link:
    command: "sudo su"
    command: "php artisan storage:link"
    cwd: "/var/app/staging"
